<h1>new post</h1>

<%= form_for :articles, url: admins_articles_path do |f| %>

	<p>
		<%= f.label :title, "title" %><br>
		<%= f.text_field :title %>
	</p>

	<p>
		<%= f.label :content, "text" %><br>
		<%= f.text_area :content %>
	</p>

	<p>
		<%= f.submit %>
	</p>
<% end %>
<%= link_to 'back', :controller => "admins/articles", :action => "index" %>
